
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<html xmlns="http://www.w3.org/1999/xhtml">

<head id="Head1" runat="server">
    <!--<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />-->
    <link href="../../../Content/Css/Screen.css" rel="stylesheet" type="text/css" media="screen" />
    <link href="../../../Content/Css/Print.css" rel="stylesheet" type="text/css" media="print" />
    <!--[if lt IE 8]><link href="${ResolveUrl("~")}/Content/Css/Ie.css" rel="stylesheet" type="text/css" /><![endif]-->
    <link href="../../../Content/Css/Site.css" rel="stylesheet" type="text/css" />
    <link href="../../../Content/Css/jquery-ui-1.8.7.custom.css" rel="stylesheet" type="text/css" />
    <script src="../../Content/Scripts/jquery-1.4.2.js" type="text/javascript"></script>
	<script type="text/javascript" src="../../Content/Scripts/jquery-ui-1.8.7.custom.min.js"></script>
</head>

<body>
    <div class="page">

        <div class="container mainContent">
        
            <div class="span-24 last">
                <div class="span-1">&nbsp;</div>
                <div class="span-10 headerLeft" id="HomeLink">&nbsp;</div>
                <div class="span-13 last headerRight">&nbsp;</div>
            </div>
            
            <div class="span-1">&nbsp;</div>
           <div class="span-22 headerBar">
                <div class="span-21 append-1 last headerBarText">
                     !{Html.TabbedMenu(new List[[ProgressTen.Infrastructure.ViewModels.MenuTab]] {
                        MenuTab.Create("Info", "Info", "Site", ""),
                        MenuTab.Create("About", "About", "Site", ""),
                        MenuTab.Create("Contact", "Contact", "Site", "")
                    })}
                </div>
            </div>

            <div class="span-1 last"></div>
            
            <div class="span-24 last mainViewArea mainHeight">
                <div class="span-1">&nbsp;</div>
                <div class="span-22">
                    <use content="view"/>
                </div>
                <div class="span-1 last">&nbsp;</div>
            </div>

            <div class="span-24 last">&nbsp;</div>

            <div class="span-1">&nbsp;</div>
            <div class="prepend-6 span-10 append-6 footer">
                <footer/>
            </div>
            <div class="span-1 last"></div>
        </div>
        <div>&nbsp;</div>
        <div>&nbsp;</div>

    </div>

    <script language="javascript" type="text/javascript">

        $(document).ready(function () {
            $("#HomeLink").hover(mouseOn, mouseOff);
            $("#HomeLink").click(function () { window.location = '${ViewData["HomeUrl"]}'; });
        });

        function mouseOn() {
            $("#HomeLink").addClass("homeHover");
        }

        function mouseOff() {
            $("#HomeLink").removeClass("homeHover");
        }

    </script>
</body>
</html>
