<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<html xmlns="http://www.w3.org/1999/xhtml">

<head id="Head1" runat="server">
	<title>@ViewBag.Title</title>

	

    @*<link href="@("http://" + Request.Url.Authority + "/Content/Css/Screen.css")" rel="stylesheet" type="text/css" media="screen" />*@
    <link href="@("http://" + Request.Url.Authority + "/Content/Css/Print.css")" rel="stylesheet" type="text/css" media="print" />
    <!--[if lt IE 8]><link href="../../Content/Css/Ie.css" rel="stylesheet" type="text/css" /><![endif]-->
    <link href="@("http://" + Request.Url.Authority + "/Content/Css/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@("http://" + Request.Url.Authority + "/Content/Css/jquery-ui-1.8.7.custom.css")" rel="stylesheet" type="text/css" />
	<link href="@("http://" + Request.Url.Authority + "/Content/Css/jquery.ui.autocomplete.css")" rel="stylesheet" type="text/css" />

    <script src="@("http://" + Request.Url.Authority + "/Content/Scripts/jquery-1.4.2.js")" type="text/javascript"></script>
	<script src="@("http://" + Request.Url.Authority + "/Content/Scripts/jquery-ui-1.8.7.custom.min.js")" type="text/javascript"></script>
</head>

<script language="javascript" type="text/javascript">

	$(document).ready(function () {
		$("#menuIconItem").click(function () {
			$("#openMenuItem").toggle();
		});

//		$("#menuContainerItem").mouseout(function () {
//			$("#openMenuItem").hide();
//		});
	});

</script>

<body>
    <div class="page">
        
            <div class="header">
				<div class="headerLatin"></div>
				<div class="headerLogo"></div>
				<div id="menuContainerItem" class="menuContainer">
					<span class="menuTitle">Menu</span>
					<div id="menuIconItem" class="menuIcon"></div>
					<div>
						<ul id="openMenuItem" class="openMenu">
							@*<li>Articles</li>*@
							<li>@Html.ActionLink("Classes", "Classes")</li>
							<li>About</li>
							<li>Contact</li>
						</ul>
					</div>
				</div>
            </div>
            
            <div class="">
				@RenderBody()
            </div>
			        
            <div class="footer">
				
            </div>

    </div>
</body>
</html>
